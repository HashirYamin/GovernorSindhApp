<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ContactUsFragment">


    <GridLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"

        android:columnCount="3"
        android:rowCount="2"
        >
        <androidx.cardview.widget.CardView
            android:id="@+id/web_link"
            android:layout_width="@dimen/_90sdp"
            android:layout_height="@dimen/_90sdp"
            android:layout_below="@id/acedamics"
            android:layout_centerHorizontal="true"
            android:layout_marginRight="@dimen/_2sdp"
            android:backgroundTint="@color/lightGreen2"
            android:layout_marginLeft="@dimen/_3sdp"
            app:cardCornerRadius="@dimen/_6sdp">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >
                <View
                    android:layout_width="@dimen/_75sdp"
                    android:layout_height="@dimen/_20sdp"
                    android:layout_marginLeft="@dimen/_2sdp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:background="@drawable/green_sh"
                    >
                </View>

                <TextView
                    android:id="@+id/w_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="Website"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:textSize="@dimen/_10sdp"
                    android:textColor="@color/white" />
                <ImageView
                    android:layout_width="@dimen/_55sdp"
                    android:layout_height="@dimen/_55sdp"
                    android:layout_below="@+id/w_txt"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/www"

                    />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/fb"
            android:layout_width="@dimen/_90sdp"
            android:layout_height="@dimen/_90sdp"
            android:layout_below="@id/acedamics"
            android:layout_marginStart="@dimen/_5sdp"
            android:layout_centerHorizontal="true"
            android:backgroundTint="@color/lightGreen2"
            android:layout_marginLeft="@dimen/_3sdp"
            app:cardCornerRadius="@dimen/_6sdp">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >
                <View
                    android:layout_width="@dimen/_75sdp"
                    android:layout_height="@dimen/_20sdp"
                    android:layout_marginLeft="@dimen/_2sdp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:background="@drawable/green_sh"
                    >
                </View>

                <TextView
                    android:id="@+id/fb_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="Facebook"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:textSize="@dimen/_10sdp"
                    android:textColor="@color/white" />
                <ImageView
                    android:layout_width="@dimen/_55sdp"
                    android:layout_height="@dimen/_55sdp"
                    android:layout_below="@+id/fb_txt"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/facebook"

                    />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/insta"
            android:layout_width="@dimen/_90sdp"
            android:layout_height="@dimen/_90sdp"
            android:layout_below="@id/acedamics"
            android:layout_marginStart="@dimen/_5sdp"
            android:layout_centerHorizontal="true"
            android:backgroundTint="@color/lightGreen2"
            android:layout_marginLeft="@dimen/_3sdp"
            app:cardCornerRadius="@dimen/_6sdp">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >
                <View
                    android:layout_width="@dimen/_75sdp"
                    android:layout_height="@dimen/_20sdp"
                    android:layout_marginLeft="@dimen/_2sdp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:background="@drawable/green_sh"
                    >
                </View>

                <TextView
                    android:id="@+id/insta_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="Instagram"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:textSize="@dimen/_10sdp"
                    android:textColor="@color/white" />
                <ImageView
                    android:layout_width="@dimen/_55sdp"
                    android:layout_height="@dimen/_55sdp"
                    android:layout_below="@+id/insta_txt"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/instagram"
                    />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/twitter"
            android:layout_width="@dimen/_90sdp"
            android:layout_height="@dimen/_90sdp"
            android:layout_below="@id/acedamics"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginStart="@dimen/_5sdp"
            android:layout_centerHorizontal="true"
            android:backgroundTint="@color/lightGreen2"
            android:layout_marginLeft="@dimen/_3sdp"
            app:cardCornerRadius="@dimen/_6sdp">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >
                <View
                    android:layout_width="@dimen/_75sdp"
                    android:layout_height="@dimen/_20sdp"
                    android:layout_marginLeft="@dimen/_2sdp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:background="@drawable/green_sh"
                    >
                </View>

                <TextView
                    android:id="@+id/twitter_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="Twitter"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:textSize="@dimen/_10sdp"
                    android:textColor="@color/white" />
                <ImageView
                    android:layout_width="@dimen/_55sdp"
                    android:layout_height="@dimen/_55sdp"
                    android:layout_below="@+id/twitter_txt"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/twitter"
                    />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/linked_in"
            android:layout_width="@dimen/_90sdp"
            android:layout_height="@dimen/_90sdp"
            android:layout_below="@id/acedamics"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginStart="@dimen/_5sdp"
            android:layout_centerHorizontal="true"
            android:backgroundTint="@color/lightGreen2"
            android:layout_marginLeft="@dimen/_3sdp"
            app:cardCornerRadius="@dimen/_6sdp">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >
                <View
                    android:layout_width="@dimen/_75sdp"
                    android:layout_height="@dimen/_20sdp"
                    android:layout_marginLeft="@dimen/_2sdp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:background="@drawable/green_sh"
                    >
                </View>

                <TextView
                    android:id="@+id/linkedin_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="LinkedIn"
                    android:layout_marginTop="@dimen/_4sdp"
                    android:textSize="@dimen/_10sdp"
                    android:textColor="@color/white" />
                <ImageView
                    android:layout_width="@dimen/_55sdp"
                    android:layout_height="@dimen/_55sdp"
                    android:layout_below="@+id/linkedin_txt"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/linkedin"
                    />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>
    </GridLayout>

</RelativeLayout>